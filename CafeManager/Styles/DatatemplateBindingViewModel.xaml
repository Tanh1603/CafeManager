<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:viewmodels="clr-namespace:CafeManager.WPF.ViewModels"
    xmlns:views="clr-namespace:CafeManager.WPF.Views">

    <DataTemplate DataType="{x:Type viewmodels:TestViewModel}">
        <views:MainAdminView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:TestImportViewModel}">
        <views:TestImportView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:TestInventoryViewModel}">
        <views:TestInventoryView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:TestSupplierViewModel}">
        <views:TestSupplierView />
    </DataTemplate>
</ResourceDictionary>