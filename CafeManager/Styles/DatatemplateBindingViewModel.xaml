<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:viewmodels="clr-namespace:CafeManager.WPF.ViewModels"
    xmlns:views="clr-namespace:CafeManager.WPF.Views">

    <DataTemplate DataType="{x:Type viewmodels:MainAdminViewModel}">
        <views:MainAdminView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:SupplierViewModel}">
        <views:SupplierListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:FoodListViewModel}">
        <views:FoodListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:HomeViewModel}">
        <views:HomeAdminView />
    </DataTemplate>
</ResourceDictionary>